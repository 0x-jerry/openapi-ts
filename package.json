{
  "name": "@0x-jerry/openapi-ts",
  "version": "1.2.2",
  "description": "This package provide a quick way to transform OpenAPI schema or Swagger schema to client typescript code.",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./runtime": {
      "default": "./dist/runtime/index.js",
      "types": "./dist/runtime/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "build": "tsup",
    "g:test": "tsx test/e2e.ts",
    "release": "x-release",
    "prepublishOnly": "npm run build"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "runtime/index.ts"
    ],
    "format": "esm",
    "dts": true,
    "clean": true
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "optionalDependencies": {
    "axios": "^1.7.2",
    "ky": "^1.3.0"
  },
  "dependencies": {
    "@0x-jerry/utils": "^2.3.0",
    "fs-extra": "^11.2.0",
    "json-schema-to-typescript": "^14.0.5",
    "lodash-es": "^4.17.21",
    "memfs": "^4.9.3",
    "openapi-typescript": "^6.7.6",
    "ora": "^8.0.1",
    "prettier": "^3.3.2",
    "quicktype-core": "^23.0.170",
    "swagger2openapi": "^7.0.8"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^2.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.5",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "axios": "^1.6.7",
    "ky": "^1.2.0",
    "tsup": "^8.1.0",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "vitest": "2.0.3"
  }
}